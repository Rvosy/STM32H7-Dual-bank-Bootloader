{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "type": "cortex-debug",
            "request": "launch",
            "name": "Bootloader: OpenOCD",
            "servertype": "openocd",
            "executable": "build/Bootloader/Bootloader.elf",
            "runToEntryPoint": "main",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32h7x.cfg"
            ]
        },
        {
        "name": "Bootloader: ST-LINK",
        "type": "cortex-debug",
        "request": "attach",
        "servertype": "stlink",
        "executable": "build/Bootloader/Bootloader.elf",
        "gdbTarget": "localhost:50000",
        "showDevDebugOutput": "raw",
        "postAttachCommands": [

        ]
        }

    ]
}